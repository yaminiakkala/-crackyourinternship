class Solution {
    public boolean isPalindrome(int x) {
        int sum=0;
        int n=x;
        while(x>0){
            int temp=x%10;
            sum =sum*10+temp;
            x/=10;
        }
        if(sum==n){
            return true;
        }
        else{
            return false;
        }
    }
}
